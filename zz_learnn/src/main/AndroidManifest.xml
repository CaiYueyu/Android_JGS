<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tsp.learn">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppLearning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity android:name=".recyclerview.CoverFlowActivity" />
        <activity android:name=".recyclerview.CustomLayoutActivity" />
        <activity android:name=".recyclerview.CustomRecycledLayoutActivity" />
        <activity android:name=".recyclerview.DiffItemActivity" />
        <activity android:name=".recyclerview.GridActivity" />
        <activity android:name=".recyclerview.LinearActivity" />
        <activity android:name=".recyclerview.RecycledFirstRemouldActivity" />
        <activity android:name=".recyclerview.RecycledSecondRemouldActivity" />
        <activity android:name=".recyclerview.RecyclerActivity" />
        <activity android:name=".recyclerview.StaggeredActivity" />
        <activity android:name=".ProgressBarActivity" />
        <activity android:name=".anim.AnimActivity" />
    </application>

</manifest>